package spongecell.guardian.rules.core
 
import spongecell.guardian.model.Person 
 
dialect "mvel"
 
/*
 * This rule checks that a person is >= 21
 */
rule "Person is 21"
    when
        $person : Person(age>=21)               
    then
        System.out.println($person.name + " is over the age of 21.");
end